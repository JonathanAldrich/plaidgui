package melerdi.gui.widgets.button;

state Button = 
	ButtonVisibility with ButtonParenting
{
	val peer;
	val name;
	
	method setButton()
	{
       	this <- UnparentedButton;
		this <- VisibleButton;
		printLine("Button's stats have been initialized");
	}
	
	
} 
