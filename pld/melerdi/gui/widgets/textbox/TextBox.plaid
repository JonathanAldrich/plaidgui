package melerdi.gui.widgets.textbox;

state TextBox = 
	TextBoxVisibility with TextBoxParenting
{
	val peer;
	val name;
	
	method setTextBox()
	{
       	this <- UnparentedTextBox;
		this <- VisibleTextBox;
		printLine("TextBox's stats have been initialized");
	}
} 