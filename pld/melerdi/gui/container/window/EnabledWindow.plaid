package melerdi.gui.container.window;
import  melerdi.gui.container.window.windowstate.*;
state EnabledWindow case of Enabling
{
	method disable()
	{
		this.peer.setEnabled(false);
		this <- DisabledWindow;
		this <- DeactivatedWindow;
	}
}